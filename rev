import java.util.Scanner;
public class reverseapp 
{
	Scanner Sc = new Scanner(System.in);
	public void rev()
	{
		int rev=0;
		int rem;
		System.out.print("enter any number and print in reverse order:");
		int n= Sc.nextInt();
		while(n!=0) 
		{
			rem=n%10;
			rev= rev*10+rem;
			n=n/10;
		
		}
		System.out.print("reverse of given number" +rev);
	}

}
